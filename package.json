{
  "name": "FirstReactNativeApp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios --verbose",
    "start": "react-native start",
    "pod": "npx pod-install",
    "port:check": "lsof -i:8081",
    "svgr": "npx @svgr/cli ./svgs -d ./src/components/svgs --native --svg-props 'className=' --svgo-config='{ \"plugins\": [{ \"removeViewBox\": false }] }'",
    "my:debug:react": "react-devtools",
    "my:release:ios": "react-native run-ios --configuration Release",
    "my:release:android": "react-native run-android --variant=release",
    "test": "jest",
    "lint": "eslint ."
  },
  "dependencies": {
    "@dudigital/react-native-zoomable-view": "^1.0.15",
    "@react-native-community/datetimepicker": "^2.3.2",
    "@react-native-community/masked-view": "^0.1.8",
    "@react-native-community/push-notification-ios": "^1.2.0",
    "@react-native-community/viewpager": "^4.0.0",
    "@react-navigation/bottom-tabs": "^5.2.7",
    "@react-navigation/native": "^5.1.5",
    "@react-navigation/stack": "^5.2.10",
    "react": "16.11.0",
    "react-native": "0.62.2",
    "react-native-actions-sheet": "^0.2.6",
    "react-native-border-zoom-view": "^1.0.4",
    "react-native-fast-image": "^8.1.5",
    "react-native-gesture-handler": "^1.6.1",
    "react-native-image-pan-zoom": "^2.1.11",
    "react-native-image-zoom-viewer": "^2.2.27",
    "react-native-linear-gradient": "^2.5.6",
    "react-native-parallax-scroll-view": "^0.21.3",
    "react-native-pinch-zoom-image": "^0.0.10",
    "react-native-push-notification": "^3.3.1",
    "react-native-reanimated": "^1.8.0",
    "react-native-safe-area-context": "^0.7.3",
    "react-native-scalable-image": "^1.0.0",
    "react-native-screens": "^2.4.0",
    "react-native-svg": "^12.1.0",
    "styled-components": "^5.1.0",
    "styled-system": "^5.1.5"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/runtime": "^7.9.2",
    "@react-native-community/eslint-config": "^1.0.0",
    "babel-jest": "^25.3.0",
    "eslint": "^6.8.0",
    "jest": "^25.3.0",
    "metro-react-native-babel-preset": "^0.59.0",
    "react-devtools": "^4.6.0",
    "react-test-renderer": "16.11.0"
  },
  "jest": {
    "preset": "react-native"
  }
}
